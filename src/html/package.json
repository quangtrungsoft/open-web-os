{
  "name": "webos",
  "version": "1.0.0",
  "description": "A fully interactive web operating system desktop environment built with HTML, CSS, and JavaScript",
  "main": "index.html",
  "scripts": {
    "start": "python -m http.server 8000",
    "dev": "live-server --port=8000",
    "build:css": "sass styles/main.scss styles/main.css",
    "watch:css": "sass --watch styles/main.scss styles/main.css",
    "build": "npm run build:css",
    "serve": "npm run dev"
  },
  "keywords": [
    "webos",
    "operating-system",
    "desktop",
    "ui",
    "web",
    "javascript",
    "css",
    "html",
    "virtual-desktop"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "live-server": "^1.2.2",
    "sass": "^1.69.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/webos.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/webos/issues"
  },
  "homepage": "https://github.com/yourusername/webos#readme"
} 